pipeline{
    agent none
    triggers{
    cron('* * * * *')
    }
    stages{
        stage ('First'){
          steps{
            echo 'First Display message'
            }
            }
            
        stage ('Second'){
            input{
            message "Hello! This is my first message box?"
            ok "Yes, Lets us go ahead"
            }
            steps{
            echo 'First Display message'
            }
            }
         }  
}
